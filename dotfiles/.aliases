# Zsh
alias reload-zsh='source ~/.zshrc'
 
# Vim
alias v='vim'

# Init Go Project
alias init-go-cli='git clone https://github.com/jroderic-07/flags-cli-template.git'

# TMux
alias t='tmux'
alias tn='tmux new -s'
alias ta='tmux attach -t'
alias tl='tmux ls'
alias tk='tmux kill-session -t'
alias tr='tmux rename-session -t'

# Get Public IP Address
alias getIP='curl ifconfig.io'

# Export Configurations
alias export-configs='cat ~/.aliases > Documents/repos/github/jroderic-07/system_tools/dotfiles/.aliases && cat ~/.vimrc > Documents/repos/github/jroderic-07/system_tools/dotfiles/.vimrc && cat ~/.tmux.conf > Documents/repos/github/jroderic-07/system_tools/dotfiles/.tmux.conf && cat ~/.zshrc > Documents/repos/github/jroderic-07/system_tools/dotfiles/.zshrc'
